/*================ Bold RO ================*/
.ro_wrapper {
  position: relative;
  margin: 0 auto;
  text-align: left;
  width: 260px;

  @include at-query ($min, $medium) {
    width: auto;
    margin: 20px 0 0 0;
  }

  @include at-query ($min, $large) {
    margin: 0;
  }

  .ro_deliver_every_label {
    position: absolute;
    top: -12px;
    left: 0;
    font-size: 12px;

    @include at-query ($min, $large) {
      font-size: 14px;
      top: -15px;
    }
  }

  .icon-chevron-down {
    position: absolute;
    display: inline-block;
    font-size: 7px;
    z-index: 1;
    top: 30px;
  }

  .icon-chevron-down:nth-child(1) {
    left: 30px;

    @include at-query ($min, $preSiteWidth) {
      left: 60px;
    }
  }

  .icon-chevron-down:nth-child(2) {
    left: 165px;

    @include at-query ($min, $preSiteWidth) {
      left: 245px;
    }
  }

  .ro_cancel {
    position: absolute;
    left: 210px;
    top: 21px;
    border-bottom: 1px solid;
    font-family: $circularBook;
    user-select: none;

    @include at-query ($min, $preSiteWidth) {
      left: 290px;
    }

    &:hover {
      cursor: pointer;
      opacity: 0.7;
    }
  }
}

.ro_widget {
  .bold-ro__recurring-title {
    display: none;
  }

  .bold-ro__one-time-div {
    display: none;
  }

  .bold-ro__recurring-lbl {
    display: none;
  }

  .bold-ro__order-interval-lbl {
    display: none;
  }

  .bold-ro__recurring-div {
    padding: 0;
    background: #fff;
    border: none;
  }

  .bold-ro__frequency-num {
    width: 60px;
    margin-right: 5px;

    @include at-query ($min, $preSiteWidth) {
      width: 90px;
    }
  }

  .bold-ro__frequency-type {
    width: 120px !important;

    @include at-query ($min, $preSiteWidth) {
      width: 170px !important;
    }
  }

  .bold-ro__frequency-num,
  .bold-ro__frequency-type {
    border-radius: 0;
    font-family: $circularBold;
    background: none;
    z-index: 5;
    position: relative;
    margin-top: 0;
    height: 48px;
    padding: 10px 10px 11px 10px;

    @include at-query ($min, $preSiteWidth) {
      padding: 10px 25px 11px 25px;
    }
  }

  .bold-ro__product {
    margin: -10px 0 -5px 0;
  }
}

