/*================ Homepage Hero ================*/
.hero {
  visibility: hidden;
  width: 100%;
  margin-top: -88px;

  @include at-query ($min, $large) {
    margin-top: -138px;
  }

  &.slick-initialized {
    visibility: visible;
  }

  &.hero--about {
    .hero__slide .hero__subline {
      right: -108px;
    }
  }

  .wrapper {
    position: relative;
    height: 100%;
    padding: 0 24px;
  }

  .slick-list {
    overflow: visible;
  }

  .hero__overlay {
    @include transition(all 0.4s cubic-bezier(.64,.05,.35,1));
    left: -120px;
    width: 219px;
    position: absolute;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    z-index: 2;
    top: 250px;
    opacity: 0;

    @include at-query ($min, $medium) {
      top: 80px;
    }

    @include at-query ($min, 1000px) {
      left: -150px;
      width: 325px;
    }

    @include at-query ($min, $large) {
      left: -200px;
      width: 419px;
    }

    @include at-query ($min, $siteWidth) {
      left: -260px;
      width: 519px;
    }

    &.active {
      opacity: 1;
    }
  }

  .hero__background {
    @include transition(all 0.7s cubic-bezier(.64,.05,.35,1));
    background-size: cover;
    background-position: center center;
    position: absolute;
    top: 0;
    left: -7.5%;
    z-index: 1;
    width: 115%;
    height: 115%;
    opacity: 0;

    @include at-query ($min, $medium) {
      background-position: top center;
    }

    &.active {
      height: 100%;
      width: 100%;
      left: 0;
      opacity: 1;
    }
  }

  .hero__slide {
    position: relative;
    height: 750px;
    width: 100%;
    overflow: hidden;

    @include at-query ($min, $large) {
      height: 700px;
    }

    @include at-query ($min, $siteWidth) {
      height: 900px;
    }

    &.complete {
      overflow: visible;
    }

    .hero__content {
      @include transition(all 0.7s cubic-bezier(.64,.05,.35,1));
      width: 100%;
      display: block;
      margin: 0 auto;
      text-align: center;
      margin-top: 70px;
      z-index: 5;
      position: relative;
      padding-top: 60px;

      @include at-query ($min, $medium) {
        max-width: 400px;
        height: 100%;
      }

      @include at-query ($min, $large) {
        margin-top: 102px;
        max-width: 540px;
        padding-top: 0;
      }

      &.hero__content--left {
        @include at-query ($min, $medium) {
          text-align: left;
          margin: 102px 0 0 0;
        }
      }

      &.hero__content--right {
        @include at-query ($min, $medium) {
          text-align: left;
          margin: 102px 0 0 0;
          float: right;
        }
      }

      &.active {
        opacity: 1;
        margin-top: 40px;

        @include at-query ($min, $large) {
          margin-top: 0;
        }
      }
    }

    .hero__headline,
    .hero__subline,
    .hero__button,
    .hero__badge {
      @include transition(all 0.2s cubic-bezier(.64,.05,.35,1));
      opacity: 0;

      &.active {
        opacity: 1;
      }
    }

    .hero__headline {
      font-family: $recoletaBold;
      color: #fff;
      font-size: 35px;
      line-height: 35px;
      margin-bottom: 0;
      letter-spacing: 0.31px;

      @include at-query ($min, $small) {
        font-size: 40px;
        line-height: 40px;
      }

      @include at-query ($min, $large) {
        font-size: 72px;
        line-height: 77px;
      }

      span {
        display: block;
      }

      .icon {
        display: inline-block;
        font-size: 18px;
        line-height: 1;
        vertical-align: top;
        margin: 4px 0 0 -2px;

        @include at-query ($min, $large) {
          font-size: 25px;
          margin: 8px 0 0 -10px;
        }
      }
    }

    .hero__subline {
      font-family: $recoletaBold;
      letter-spacing: 0.31px;
      margin-top: 20px;
      color: #fff;
      font-size: 20px;

      @include at-query ($max, $preMedium) {
        position: absolute;
        right: -40px;
        bottom: -180px;
        transform: rotate(-90deg);
      }

      @include at-query ($min, $large) {
        font-size: 24px;
        margin-top: 40px;
      }

      .icon {
        display: inline-block;
        font-size: 13px;
        line-height: 1;
        vertical-align: top;

        @include at-query ($min, $small) {
          margin-top: 4px;
        }

        @include at-query ($min, $large) {
          margin-top: 6px;
        }
      }
    }

    .hero__button {
      margin-top: 15px;
      display: inline-block;
      padding: 12px 20px 13px 20px;
      line-height: 1;
      font-size: 16px;
      font-family: $circularBold;

      @include at-query ($min, $medium) {
        font-size: 18px;
        margin-top: 30px;
        padding: 16px 35px 15px 35px;
      }

      &:hover {
        opacity: 0.75;
      }
    }

    .hero__badge {
      position: absolute;
      right: 20px;
      bottom: -20px;
      width: 90px;
      z-index: 8;

      @include at-query ($min, $large) {
        bottom: -48px;
        width: 138px;
      }
    }
  }
}

.badge-text {
  -webkit-animation: rotating 20s linear infinite;
  -moz-animation: rotating 20s linear infinite;
  -ms-animation: rotating 20s linear infinite;
  -o-animation: rotating 20s linear infinite;
  animation: rotating 20s linear infinite;
  transform-origin: center center;
}

@-webkit-keyframes rotating /* Safari and Chrome */ {
  from {
    -webkit-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@keyframes rotating {
  from {
    -ms-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  to {
    -ms-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

