$circularBold: 'Circular STD Bold', Helvetica, Arial, sans-serif;
$circularMedium: 'Circular STD Medium', Helvetica, Arial, sans-serif;
$circularBook: 'Circular STD Book', Helvetica, Arial, sans-serif;
$recoletaBold: 'Recoleta Bold', Helvetica, Arial, sans-serif;
$aprecuMono: 'Aprecu Mono Pro Regular', Helvetica, Arial, sans-serif;
$aprecuMonoLight: 'Aprecu Mono Pro Light', Helvetica, Arial, sans-serif;

body {
  font-size: 16px;
  font-family: $aprecuMonoLight;
}

input, textarea, button, select, label {
  font-size: 16px;
  font-family: $aprecuMonoLight;
}

h1,h2,h3,h4,h5,h6,
.h1,.h2,.h3,.h4,.h5,.h6 {
  font-family: $recoletaBold;
  font-weight: normal;
}

h1 {
  font-size: 25px;

  @include at-query ($min, $medium) {
    font-size: 32px;
    margin-bottom: 35px;
  }
}

.rte {
  h2,h3,h4,h5,h6 {
    font-family: $circularBold;
  }

  p {
    margin-bottom: 25px;
  }
}

/*================ General ================*/
@mixin transition($transition) {
  -moz-transition: $transition;
  -o-transition: $transition;
  -webkit-transition: $transition;
  transition: $transition;
}

a, button, .btn {
  @include transition(all 0.2s cubic-bezier(.64,.05,.35,1));
}

.btn, .btn--secondary {
  font-family: $circularBold;
  padding: 14px 25px;
}

body {
  @include transition(opacity 0.4s cubic-bezier(0.44, 0.13, 0.48, 0.87));
}

.no-js {
  .fade-in {
    opacity: 1;
    padding-top: 0;
  }
}

.fade-in {
  @include transition(all 0.4s cubic-bezier(0.44, 0.13, 0.48, 0.87));
  opacity: 0;

  &.fade-in--active {
    opacity: 1;
  }
}

.fade-in-on-load {
  @include transition(all 0.4s cubic-bezier(0.44, 0.13, 0.48, 0.87));
  opacity: 0;

  &.fade-in-on-load--active {
    opacity: 1;
  }
}

input, textarea, select {
  padding: 12px 25px 11px 25px;
  border-radius: 0;
}

button, .btn {
  height: 48px;
}

/*================ Promobar ================*/
.promobar {
  text-align: center;
  padding: 8px 0;
  font-family: $circularBold;
  font-size: 14px;
  line-height: 16px;
  letter-spacing: -0.5px;
  z-index: 100;
  position: relative;

  @include at-query ($min, $medium) {
    font-size: 16px;
    line-height: 32px;
    line-height: 32px;
    padding: 0;
    height: 33px;
  }
}

/*================ Header ================*/
.site-header {
  position: relative;
  padding: 20px 0;
  z-index: 15;

  @include at-query ($min, $large) {
    padding: 40px 0;

    &:before {
      content: '';
      display: block;
      width: 100%;
      height: 100%;
      height: 45px;
      z-index: 20;
      position: absolute;
      top: 0;
      left: 0;
    }
  }

  .site-nav--mobile {
    display: inline-block;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    z-index: 20;

    &.site-nav__menu {
      left: 10px;

      .site-nav__link {
        font-size: 24px;
      }
    }

    &.site-nav__cart {
      right: 10px;

      .site-nav__link {
        font-size: 20px;
      }
    }

    .site-nav__link {
      line-height: 1;
      padding: 14px 12px;
    }
  }

  .site-header__logo {
    position: relative;
    z-index: 15;

    a {
      display: inline-block;
    }

    svg {
      @include transition(all 0.2s cubic-bezier(.64,.05,.35,1));
      width: 120px;
      fill: $colorPrimary;

      @include at-query ($min, $large) {
        width: auto;
        margin-top: -9px;
      }
    }
  }

  .site-header__search {
    &:hover {
      cursor: pointer;
    }
  }

  .icon-search {
    font-size: 30px;
    margin-right: 15px;
  }

  .icon-account {
    font-size: 32px;
    margin-right: 15px;
  }

  .icon-cart {
    font-size: 32px;
  }

  a.js-drawer-open-right {
    position: relative;
    color: $colorPrimary;

    .cart-count {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      font-family: $circularBold;
      font-size: 14px;
      letter-spacing: -0.28px;
      margin-top: 3px;

      @include at-query ($min, $large) {
        margin-top: -3px;
      }
    }
  }

  .site-header__controls {
    margin-top: 5px;
    z-index: 15;
    position: relative;

    a {
      color: $colorPrimary;

      &:hover {
        opacity: 0.7;
      }
    }
  }
}

.js-drawer-open-left svg g  {
  fill: $colorPrimary;
}

.mobile-sticky-header {
  @include transition(all 0.3s cubic-bezier(.64,.05,.35,1));
  position: fixed;
  top: -200px;
  left: 0;
  width: 100%;
  background-color: #fff;
  display: block;
  z-index: 100;

  &.active {
    top: 0;
  }

  // .large--four-twelfths {
  //   @include at-query ($min, $siteWidth) {
  //     width: 25% !important;
  //   }
  // }
}


/*================ Header ================*/
.nav-bar {
  &.nav-bar--primary {
    ul {
      text-align: right;

      @include at-query ($min, $siteWidth) {
        text-align: left;
      }
    }

    li:last-child {
      .site-nav__link {
        margin-right: 40px;

        @include at-query ($min, $siteWidth) {
          margin-right: 0;
        }
      }
    }

    li:first-child {
      .site-nav__link {
        @include at-query ($min, $siteWidth) {
          margin-left: 35px;
        }
      }
    }
  }

  &.nav-bar--secondary {
    li:first-child {
      .site-nav__link {
        margin-left: 40px;

        @include at-query ($min, $preSiteWidth) {
          margin-left: 70px;
        }
      }
    }
  }

  li:first-child {
    .site-nav__link {
      padding-left: 0;
    }
  }

  .site-nav--has-dropdown {
    position: static;

    &:hover {
      .icon-chevron-down {
        transform: scaleY(-1);
      }

      .site-nav__dropdown {
        opacity: 1;
        margin-top: -100px;
        visibility: visible;
      }
    }

    .icon-chevron-down {
      display: inline-block;
      transform-origin: center center;
    }
  }

  .site-nav__link {
    font-size: 16px;
    letter-spacing: -0.36px;
    font-family: $circularBold;
    padding: 15px 6px;
    color: $colorPrimary;
    position: relative;
    z-index: 15;

    @include at-query ($min, $preSiteWidth) {
      font-size: 18px;
      padding: 15px;
    }
  }

  a {
    &:hover {
      opacity: 0.7;
    }
  }

  .site-nav__dropdown {
    @include transition(all 0.2s cubic-bezier(.64,.05,.35,1));
    width: 100%;
    margin-top: -200px;
    padding: 140px 0 50px 0;
    z-index: 10;
    display: block;
    visibility: hidden;
    opacity: 0;

    .wrapper {
      margin: 0 auto;

      @include at-query ($min, $preSiteWidth) {
        padding: 0 100px;
      }
    }

    h3 {
      font-family: $circularMedium;
      letter-spacing: 0.07px;
      font-size: 18px;
      margin-bottom: 33px;
      text-align: left;
    }

    ul {
      margin: 0;
      text-align: left;

      li {
        margin: 0;
      }
    }

    .scents {
      a {
        display: block;
        border-radius: 200px;
        color: #fff;
        width: 74px;
        height: 74px;
        line-height: 74px;
        font-size: 12px;
        text-align: center;
        font-family: $circularMedium;
        border-bottom: none;

        @include at-query ($min, $preSiteWidth) {
          width: 88px;
          height: 88px;
          line-height: 88px;
          font-size: 14px;
        }

        &:hover {
          color: #fff !important;
          border: none !important
        }

        &.unscented {
          background-color: #37549F;
        }

        &.tea-tree {
          background-color: #F6D258;
        }

        &.lemongrass {
          background-color: #9FC735;
        }

        &.peppermint {
          background-color: #25A787;
        }

        &.citrus {
          background-color: #F58D6F;
        }

        &.lavender {
          background-color: #CED1F4;
        }

        &.rose {
          background-color: #EF4060;
        }

        &.coconut {
          background-color: #80D3ED;
        }

        &.patchouli {
          background-color: #AC5F99;
        }
      }
    }

    a {
      color: $black;
      font-size: 14px;
      display: inline-block;
      margin-bottom: 8px;
      clear: both;
      border-bottom: 1px solid #fff;

      &:hover {
        color: #24A686;
        border-color: #24A686;
        opacity: 1;
      }
    }
  }
}

/*================ Drawers ================*/
@include at-query ($max, $preLarge) {
  html.js-drawer-open,
  body.js-drawer-open {
    overflow: visible;
    height: auto;
  }

  .drawer {
    overflow: visible;
  }

  .drawer .drawer__inside {
    overflow: visible;
  }

  body.js-drawer-open-left {
    button.js-drawer-open-left {
      display: none;
    }

    .open-right-overlay {
      display: block;
      width: 60px;
      height: 60px;
      position: absolute;
      right: 0;
      top: 0;
    }
  }

  body.js-drawer-open-right {
    a.js-drawer-open-right {
      display: none;
    }

    .open-left-overlay {
      display: block;
      width: 60px;
      height: 60px;
      position: absolute;
      left: 0;
      top: 0;
    }
  }
}

.js-drawer-open #PageContainer {
  @include at-query ($min, $medium) {
    height: auto !important;
  }
}

.drawer {
  background-color: #fff;
  font-family: $aprecuMonoLight;
  top: 0;
  color: $black;
  padding: 0 22px;

  @include at-query ($max, $preLarge) {
    padding-top: 19px;
    z-index: 1000;
  }

  @include at-query ($min, $large) {
    top: 0;
    height: 100% !important;
    padding-top: 24px;
  }

  &.promobar-active {
    padding: 0;

    .drawer__header {
      padding: 19px 22px;
    }
  }

  &.drawer--left {
    height: 1000px;

    @include at-query ($min, $medium) {
      height: 645px;
    }

    .drawer__header {
      .drawer__close {
        button {
          margin-top: -12px;
        }
      }
    }
  }

  &.drawer--left.promobar-active {
    .drawer__header {
      .drawer__close {
        button {
          margin-top: 2px;
        }
      }
    }
  }

  &.drawer--right {
    min-height: 700px;

    @include at-query ($max, $preLarge) {
      height: 700px;
    }

    .drawer__header {
      margin-bottom: 20px;

      .drawer__close {
        button {
          margin-top: -12px;
        }
      }
    }
  }

  &.drawer--right.promobar-active {
    .drawer__header {
      .drawer__close {
        button {
          margin-top: 8px;
        }
      }
    }
  }

  .drawer__menu {
    position: absolute;
    margin-top: 15px;
    z-index: 10;

    svg g {
      fill: $colorPrimary;
    }
  }

  .drawer__cart {
    position: relative;
    color: $colorPrimary;
    float: right;
    z-index: 10;
    margin: 2px 0 0 0;

    @include at-query ($min, 399px) {
      margin: 3px 0 0 0;
    }

    .icon-cart {
      font-size: 32px;
    }

    .cart-count {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      font-family: $circularBold;
      font-size: 14px;
      letter-spacing: -0.28px;
      margin-top: 0;
    }
  }

  .drawer__logo {
    position: absolute;
    width: 100%;
    left: 0;
    text-align: center;
    margin: 1px auto 0 auto;
    z-index: 1;

    a {
      display: inline-block;
    }

    svg {
      @include transition(all 0.2s cubic-bezier(.64,.05,.35,1));
      width: 120px;
      fill: $colorPrimary;

      @include at-query ($min, $large) {
        width: auto;
        margin-top: -9px;
      }
    }
  }

  .drawer__close {
    position: relative;
    z-index: 10;
  }

  .drawer__header {
    border: none;

    .drawer__title {
      color: $colorPrimary;
      font-family: $circularBold;
      padding-top: 5px;
    }

    .drawer__close {
      color: $colorPrimary;
      text-align: right;

      button {
        font-size: 25px;
        right: 0;
        padding: 0;
        margin-top: 0;

        @include at-query ($min, $large) {
          right: 45px;
        }

        .path1:before {
          color: $colorPrimary;
        }

        .path2:before {
          color: #fff;
        }
      }
    }
  }

  .search-bar {
    margin-bottom: 40px;
    margin: 0 22px 40px 22px;
  }

  .drawer__top-links {
    text-align: center;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    border-bottom: 1px solid $colorPrimary;
    margin: 0 22px 20px 22px;
    padding-bottom: 16px;

    a {
      font-family: $circularBold;
      letter-spacing: -0.32;
      font-size: 16px;
      color: $colorPrimary;
    }
  }

  .drawer__nav {
    padding: 0 22px;

    .grid__item {
      margin-bottom: 22px;
    }

    h3 {
      font-family: $circularMedium;
      color: $black;
      letter-spacing: 0.05px;
      font-size: 12px;
      margin-bottom: 15px;

      @include at-query ($min, $medium) {
        font-size: 15px;
      }
    }

    a {
      font-family: $aprecuMonoLight;
      font-size: 13px;
      color: $black;
      margin-bottom: 13px;
      display: block;

      @include at-query ($min, $medium) {
        font-size: 14px;
      }
    }
  }

  .icon-plus,
  .icon-minus {
    font-size: 16px;
  }

  .mobile-nav__item {
    background: none;

    &:after {
      border-color: #f1f1f1;
    }

    &.mobile-nav__item-last {
      margin-bottom: 20px;
    }

    &.mobile-nav__search {
      input {
        border: none;
      }
    }

    a {
      color: $colorPrimary;
      font-size: 20px;
      font-family: $circularBold;
      padding: 12px 10px 12px 12px;
    }

    .mobile-nav__toggle-open,
    .mobile-nav__toggle-close {
      color: $colorPrimary;
      line-height: 0;

      &:active,
      &:hover,
      &:focus {
        background: none;
      }
    }

    .mobile-nav__sublist {
      li {
        &:after {
          border: none;
        }

        &:last-child {
          margin-bottom: 25px;
        }

        a {
          padding: 3px 30px;
          font-family: $aprecuMonoLight;
          font-size: 16px;
        }
      }
    }
  }

  .mobile-nav__sublink {
    &:after {
      border: none;
    }

    &.mobile-nav__sublink--last {
      margin-bottom: 25px;
    }

    a {
      font-size: 15px;
      font-family: $aprecuMonoLight;
      color: $colorPrimary;
      padding: 2px 0 2px 15px;
    }
  }

  #CartContainer {
    max-width: 360px;
    margin: 0 auto;
    padding: 0 14px;

    @include at-query ($min, 399px) {
      padding: 0;
    }

    .ajaxcart__continue,
    .ajaxcart__checkout {
      padding: 10px 25px;
      font-size: 18px;
    }
  }

  .ajaxcart__row {
    border-bottom: 2px solid #E9E9E9;
    padding-bottom: 24px;
    margin-bottom: 20px;

    .ajaxcart__product-image {
      @include transition(none);
      height: 100px;
      margin: 4px 0 0 0;
      display: block;

      @include at-query ($min, $large) {
        height: 120px;
      }
    }

    img {
      max-height: 70px;
      width: auto;

      @include at-query ($min, $large) {
        max-height: 90px;
      }
    }

    .ajaxcart__product-meta {
      padding-top: 0;
      font-size: 14px;
      margin-top: -5px;

      &.has-type {
        margin-top: 0;
      }
    }

    .ajaxcart__product-name {
      margin-bottom: 6px;
      color: $black;

      @include at-query ($min, $large) {
        font-size: 18px;
        line-height: 24px;
        margin-bottom: 10px;
      }
    }

    span {
      color: $black;
    }

    p {
      color: $black;
      margin-bottom: 3px;
    }

    a {
      color: $colorPrimary;
      font-family: $circularBold;
    }

    .ajaxcart-item__price-wrap {
      vertical-align: top;
      padding-top: 14px;
      font-size: 14px;
      font-family: $aprecuMono;
    }

    .ajaxcart__qty {
      margin-top: 7px;
      max-width: 90px;

      button {
        height: auto;
      }

      .icon {
        color: #ADAEAE;
        font-size: 13px;
      }
    }

    .ajaxcart__qty button, .ajaxcart__qty input {
      color: #ADAEAE;
      background: none;
      font-family: $circularBold;
    }

    .ajaxcart__qty button {
      padding: 5px 10px;
      font-size: 20px;
      margin-top: -2px;
    }

    .ajaxcart__qty input {
      border: 2px solid #E9E9E9;
      padding-bottom: 3px;
      padding: 2px 25px;
      font-size: 14px;
    }
  }

  .ajaxcart__qty-remove {
    margin-top: 10px;
    font-size: 14px;
    color: $colorPrimary;
    border-bottom: 1px solid $colorPrimary;
    line-height: 16px;
    height: auto;

    &:hover {
      opacity: 0.7;
    }
  }

  .ajaxcart__subtotal,
  .ajaxcart__subtotal-price {
    font-family: $circularBold;
  }

  .ajaxcart__shipping-message {
    margin-bottom: 18px;
    line-height: 21px;
  }

  .ajaxcart__checkout {
    margin-top: 5px;
  }

  .ajaxcart__empty {
    color: $black;
    text-align: center;
    font-size: 14px;

    @include at-query ($min, $large) {
      font-size: 18px;
    }
  }

  .ajaxcart__continue {
    background: none;
    border: 2px solid $colorPrimary;
    color: $colorPrimary;
    width: 100%;
    margin: 10px 0 55px 0;

    @include at-query ($min, $large) {
      margin: 20px 0 55px 0;
    }

